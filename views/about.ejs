
<div class="about-container">
    <div class="course_details">
        <div class="code-title">
            <h1 id ="ccid"><%-ccid%></h1>
            <h2 id ="title"><%-title%></h2> 
        </div>

        <h4>Department: </h4> 
        <p id="department"><%-department%></p>

        <h4>Credits: </h4> 
        <p id="credits"><%-credits%></p>

        <h4>Class description: </h4>
        <p id="description"><%-description%></p>

        <h4>Can be audited</h4>
        <p id="audit"><%-audit%></p>
        
        <h4>Course Learning Outcomes</h4>
        <p id="clos"><%-clos%></p>

        <h4>Prerequesits</h4>
        <%if (prereq) {%>
            <p id="prereq"><%-prereq%></p> 
        <%}else {%>
            <p id="prereq">None</p> 
        <%}%>
    </div>
    
    <div class="course_reviews">
        <%if (reviews) {%>
            <%-reviews%>
        <%}else {%>
            <p>No reviews yet</p> 
            <a href="/forms/rateClassForm">Be the first one to review this class!</a>
        <%}%>
    </div>
</div>